; Definition file for player
; Contains all the filenames needed for the character

; Player information
[Info]
name = "G"        ;Name of character
displayname = "G" ;Name of character to display
versiondate =    ;Version date of character
mugenversion =   ;Version of M.U.G.E.N character works on
author = ""      ;Character author name
pal.defaults =   ;Default palettes in order of preference

; Files for the player
[Files]
cmd     = G.cmd   ;Command set
cns     = G.cns   ;Constants
st      = G.cns   ;States
stcommon = common1.cns ;Common states
sprite  = G.sff   ;Sprite
anim    = G.air   ;Animation
;sound  = player.snd   ;Sound
pal1   = pal1.act   ;Palettes (can have up to 12)
pal2   = pal2.act
pal3   = pal3.act
pal4   = pal4.act
pal5   = pal5.act
pal6   = pal6.act
;pal7   =
;pal8   =
;pal9   =
;pal10  =
;pal11  =
;pal12  =
